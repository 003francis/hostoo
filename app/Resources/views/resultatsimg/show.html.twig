{% extends 'base.html.twig' %}

{% block body %}
    <h1>ResultatsImg</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resultatsImg.id }}</td>
            </tr>
            <tr>
                <th>Dateresultat</th>
                <td>{% if resultatsImg.dateresultat %}{{ resultatsImg.dateresultat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Resultat</th>
                <td>{{ resultatsImg.resultat }}</td>
            </tr>
            <tr>
                <th>Fichier</th>
                <td>{{ resultatsImg.fichier }}</td>
            </tr>
            <tr>
                <th>Attente</th>
                <td>{% if resultatsImg.attente %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resultatsimg_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('resultatsimg_edit', { 'id': resultatsImg.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
