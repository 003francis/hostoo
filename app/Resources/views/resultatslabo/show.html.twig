{% extends 'hostooSanteBundle::blank.html.twig' %}

{% block body %}
    <h1>ResultatsLabo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resultatsLabo.id }}</td>
            </tr>
            <tr>
                <th>Dateresultat</th>
                <td>{% if resultatsLabo.dateresultat %}{{ resultatsLabo.dateresultat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Resultat</th>
                <td>{{ resultatsLabo.resultat }}</td>
            </tr>
            <tr>
                <th>Attente</th>
                <td>{% if resultatsLabo.attente %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resultatslabo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('resultatslabo_edit', { 'id': resultatsLabo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
