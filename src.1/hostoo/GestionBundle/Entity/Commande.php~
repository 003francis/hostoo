<?php

namespace hostoo\GestionBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Commande
 *
 * @ORM\Table(name="commande")
 * @ORM\Entity(repositoryClass="hostoo\GestionBundle\Repository\CommandeRepository")
 */
class Commande
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="datecmd", type="datetime")
     */
    private $datecmd;

    /**
     *
     * @ORM\ManyToOne(targetEntity="hostoo\SanteBundle\Entity\Facture", inversedBy="commandes")
     */
    private $facture;

    /**
     *
     *
     * @ORM\OneToMany(targetEntity="Tarif", mappedBy="commande")
     */
    private $tarifs;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set datecmd
     *
     * @param \DateTime $datecmd
     * @return Commande
     */
    public function setDatecmd($datecmd)
    {
        $this->datecmd = $datecmd;

        return $this;
    }

    /**
     * Get datecmd
     *
     * @return \DateTime 
     */
    public function getDatecmd()
    {
        return $this->datecmd;
    }
}
