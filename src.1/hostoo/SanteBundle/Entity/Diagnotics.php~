<?php

namespace hostoo\SanteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Diagnotics
 *
 * @ORM\Table(name="diagnotics")
 * @ORM\Entity(repositoryClass="hostoo\SanteBundle\Repository\DiagnoticsRepository")
 */
class Diagnotics
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var array
     *
     * @ORM\ManyToMany(targetEntity="Ramsey\TagBundle\Entity\Tag", cascade={"persist"})
     */
    private $hypothesesDiagnostiques;

    /**
     * @var array
     *
     * @ORM\ManyToMany(targetEntity="Ramsey\TagBundle\Entity\Tag", cascade={"persist"})
     */
    private $Diagnostiques;

    /**
     * @ORM\OneToOne(targetEntity="Episode", inversedBy="diag")
     */
    private $episode;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }




    /**
     * Set episode
     *
     * @param \hostoo\SanteBundle\Entity\Episode $episode
     * @return Diagnotics
     */
    public function setEpisode(\hostoo\SanteBundle\Entity\Episode $episode = null)
    {
        $this->episode = $episode;

        return $this;
    }

    /**
     * Get episode
     *
     * @return \hostoo\SanteBundle\Entity\Episode 
     */
    public function getEpisode()
    {
        return $this->episode;
    }


    /**
     * Constructor
     */
    public function __construct()
    {
        $this->hypothesesDiagnostiques = new \Doctrine\Common\Collections\ArrayCollection();
        $this->Diagnostiques = new \Doctrine\Common\Collections\ArrayCollection();
    }

}
